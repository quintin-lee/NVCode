{
  "bashls": {
    "enabled": true,
    "settings": {}
  },
  "lua_ls": {
    "enabled": true,
    "settings": {
      "Lua": {
        "runtime": {
          "version": "LuaJIT"
        },
        "diagnostics": {
          "globals": ["vim", "use", "client", "bufnr"]
        },
        "workspace": {
          "library": []
        },
        "telemetry": {
          "enable": false
        }
      }
    }
  },
  "pylsp": {
    "enabled": true,
    "settings": {
      "pylsp": {
        "plugins": {
          "black": {
            "enabled": false
          },
          "autopep8": {
            "enabled": false
          },
          "yapf": {
            "enabled": false
          },
          "pycodestyle": {
            "maxLineLength": 120,
            "indentSize": 4,
            "ignore": ["E302", "E305", "E121", "E126", "W504", "W503"]
          }
        }
      }
    }
  },
  "clangd": {
    "enabled": true,
    "settings": {}
  },
  "cmake": {
    "enabled": true,
    "settings": {}
  },
  "rust_analyzer": {
    "enabled": true,
    "settings": {}
  },
  "gopls": {
    "enabled": true,
    "settings": {}
  }
}
